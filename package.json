{
  "name": "com.geuneda.uiservice",
  "displayName": "Geuneda UI Service",
  "author": "geuneda",
  "version": "1.0.0",
  "unity": "6000.0",
  "license": "MIT",
  "description": "Unity 게임 UI를 관리하는 서비스 패키지입니다. UI의 열기, 닫기, 로드, 언로드 기능을 제공하며, UI Set을 통해 여러 UI를 동시에 관리할 수 있습니다.",
  "dependencies": {
    "com.unity.addressables": "2.6.0",
    "com.cysharp.unitask": "2.5.10"
  },
  "type": "library",
  "hideInEditor": false,
  "samples": [
    {
      "displayName": "Basic UI Flow",
      "description": "기본 UI 생명주기: 로드, 열기, 닫기, 언로드",
      "path": "Samples~/BasicUiFlow"
    },
    {
      "displayName": "Data Presenter",
      "description": "데이터 기반 UI 예제",
      "path": "Samples~/DataPresenter"
    },
    {
      "displayName": "UI Sets",
      "description": "여러 UI를 그룹으로 관리하는 예제",
      "path": "Samples~/UiSets"
    }
  ]
}
